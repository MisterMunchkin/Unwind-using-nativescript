<Page class="page-content" loaded="onloaded">
    <StackLayout orientation="vertical">
        <Label class="page-icon fa" text="&#xf1ea;"></Label>
        <Label class="page-placeholder" text="booking page"></Label>

        <Label text="{{ reservationDate }}" id="resDateLabel"></Label>
        <Button text="{{ cancelText }}" id="cancelBookingID" tap="cancelButton"/>
        <Button text="Check In" tap="checkinButton" id="checkinButtonID" isEnabled = "{{ checkinisEnabled }}"/>
    </StackLayout>
</Page>